{
  "private": true,
  "name": "@lib/schema-validator",
  "version": "0.0.1",
  "author": "Elias Mawa <elias@mawa.dev>",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    "vehicle": "./dist/cjs/schema/vehicle/index.js"
  },
  "typesVersions": {
    "*": {
      "deal": [
        "./dist/cjs/schema/deal/index.js"
      ],
      "product": [
        "./dist/cjs/schema/product/index.js"
      ],
      "vehicle": [
        "./dist/cjs/schema/vehicle/index.js"
      ]
    }
  },
  "scripts": {
    "lint": "echo 'lint @lib/schema-validator' && eslint . --ext .ts",
    "prettier": "yarn prettier --write",
    "clean": "",
    "rollup": "echo 'bundle @lib/schema-validator' && rollup -c --bundleConfigAsCjs",
    "rollup:watch": "nodemon --watch 'src/' --delay 200ms -e ts --exec yarn rollup",
    "build": "NODE_ENV=production yarn build:cjs",
    "build:cjs": "echo 'build @lib/schema-validator:cjs' && tsc",
    "build:watch": "nodemon --watch 'src/' -e ts --exec yarn build",
    "test": "NODE_ENV=production jest"
  },
  "dependencies": {
    "@lib/config": "~0.0.1",
    "@lib/shared": "~0.0.1",
    "date-fns": "~2.28.0",
    "date-fns-tz": "~2.0.0",
    "form-data": "~4.0.0",
    "yup": "~1.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "~15.1.0",
    "@rollup/plugin-terser": "~0.4.3",
    "@rollup/plugin-typescript": "~11.1.2",
    "rollup": "~3.26.2",
    "rollup-plugin-dts": "~5.3.0",
    "ts-node": "~10.9.1",
    "typescript": "~5.1.6"
  }
}
